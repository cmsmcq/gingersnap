{ Grammar G_1a }
url: scheme, -":", authority, path.

@scheme: (["a"-"z"]; ["A"-"Z"]; ["0"-"9"])+.

authority: -"//", host.
host: ((["a"-"z"]; ["A"-"Z"]; ["0"-"9"])+), (".", ((["a"-"z"]; ["A"-"Z"]; ["0"-"9"])+))*.
{ host: ((["a"-"z"]; ["A"-"Z"]; ["0"-"9"])+) ++ ".". }

path: ("/", (["a"-"z"]; ["A"-"Z"]; ["0"-"9"]; ".")*)+.


